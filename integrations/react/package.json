{
  "name": "@pinceau/react",
  "version": "1.0.0-beta.26",
  "homepage": "https://github.com/Tahul/pinceau#readme",
  "bugs": {
    "url": "https://github.com/Tahul/pinceau/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tahul/pinceau.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.cjs"
    },
    "./transforms": {
      "types": "./dist/transforms.d.ts",
      "import": "./dist/transforms.mjs",
      "require": "./dist/transforms.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs"
    },
    "./unplugin": {
      "types": "./dist/unplugin.d.ts",
      "import": "./dist/unplugin.mjs",
      "require": "./dist/unplugin.cjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "plugin": [
        "./dist/plugin.d.ts"
      ],
      "transform": [
        "./dist/transform.d.ts"
      ],
      "runtime": [
        "./dist/runtime.d.ts"
      ],
      "utils": [
        "./dist/utils.d.ts"
      ],
      "unplugin": [
        "./dist/unplugin.d.ts"
      ]
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "unbuild",
    "clean": "rm -rf dist",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@pinceau/core": "workspace:*",
    "@pinceau/integration": "workspace:*",
    "@pinceau/outputs": "workspace:*",
    "acorn": "^8.11.2",
    "jsx-ast-utils": "^3.3.5",
    "ultrahtml": "npm:@pinceau/ultrahtml@1.5.2"
  },
  "devDependencies": {
    "@pinceau/configs": "workspace:*"
  }
}
