{
  "name": "pinceau",
  "version": "0.2.0",
  "packageManager": "pnpm@7.1.1",
  "description": "A CSS-in-JS framework built to feel like a native Vue feature.",
  "license": "MIT",
  "homepage": "https://github.com/Tahul/pinceau#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tahul/pinceau.git"
  },
  "bugs": {
    "url": "https://github.com/Tahul/pinceau/issues"
  },
  "keywords": [
    "css-in-js",
    "vue",
    "design-tokens",
    "css-variables",
    "typed-styles",
    "nuxt",
    "vite"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.mjs"
    },
    "./runtime": {
      "require": "./dist/runtime.cjs",
      "import": "./dist/runtime.mjs"
    },
    "./volar": "./volar/index.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "volar"
  ],
  "scripts": {
    "build": "unbuild",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "build:play": "npm -C playground run build",
    "build:play:nuxt": "npm -C playground run build:nuxt",
    "play": "npm -C playground run dev",
    "play:nuxt": "npm -C playground run dev:nuxt",
    "prepublishOnly": "npm run build",
    "release": "release-it",
    "start": "esno src/index.ts",
    "test": "vitest",
    "test:nuxt": "(cd test/fixtures/nuxt && npx nuxi dev)",
    "test:vite": "(cd test/fixtures/vite && npx vite dev)"
  },
  "dependencies": {
    "@types/babel__core": "^7.1.19",
    "@unocss/reset": "^0.45.25",
    "@volar/vue-language-core": "^0.40.13",
    "chroma-js": "^2.4.2",
    "consola": "^2.15.3",
    "csstype": "^3.1.1",
    "defu": "^6.1.0",
    "magic-string": "^0.26.4",
    "postcss-custom-properties": "^12.1.9",
    "postcss-dark-theme-class": "^0.7.3",
    "postcss-nested": "^5.0.6",
    "recast": "^0.21.5",
    "scule": "^0.3.2",
    "style-dictionary-esm": "^1.0.14",
    "unbuild": "^0.8.11",
    "unplugin": "^0.9.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@nuxt/kit": "3.0.0-rc.11",
    "@nuxt/test-utils": "3.0.0-rc.11",
    "@nuxtjs/color-mode": "^3.1.6",
    "@types/chroma-js": "^2.1.4",
    "@types/node": "^18.7.23",
    "@vue/test-utils": "^2.0.2",
    "eslint": "^8.24.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "jsdom": "^20.0.0",
    "nodemon": "^2.0.20",
    "playwright": "^1.26.1",
    "release-it": "^15.4.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.23.4",
    "vue": "^3.2.39",
    "webpack": "^5.74.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss",
        "postcss-*"
      ]
    }
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "pnpm build"
      ]
    },
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
