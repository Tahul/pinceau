{
  "name": "pinceau",
  "type": "module",
  "version": "0.10.11",
  "packageManager": "pnpm@7.20.0",
  "workspaces": [
    "docs",
    "playground"
  ],
  "description": "A CSS-in-JS framework built to feel like a native Vue feature.",
  "license": "MIT",
  "homepage": "https://github.com/Tahul/pinceau#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tahul/pinceau.git"
  },
  "bugs": {
    "url": "https://github.com/Tahul/pinceau/issues"
  },
  "keywords": [
    "css-in-js",
    "vue",
    "design-tokens",
    "css-variables",
    "typed-styles",
    "nuxt",
    "vite"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./types": {
      "require": "./dist/index.d.ts",
      "import": "./dist/index.d.ts"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs"
    },
    "./nuxt": {
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.mjs"
    },
    "./runtime": {
      "require": "./dist/runtime.cjs",
      "import": "./dist/runtime.mjs"
    },
    "./nitro": {
      "require": "./dist/nitro.cjs",
      "import": "./dist/nitro.mjs"
    },
    "./transform": {
      "require": "./dist/transform.cjs",
      "import": "./dist/transform.mjs"
    },
    "./volar": "./dist/volar.cjs"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist",
    "volar"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "pnpm -C playground run dev",
    "lint": "eslint . --cache",
    "lint:fix": "pnpm run lint --fix",
    "build:play": "pnpm -C playground run build",
    "build:play:nuxt": "pnpm -C playground run build:nuxt",
    "play": "pnpm -C playground run dev",
    "play:nuxt": "pnpm -C playground run dev:nuxt",
    "docs": "pnpm -C docs run dev",
    "build:docs": "pnpm -C docs run build",
    "prepublishOnly": "pnpm run build",
    "release": "release-it",
    "start": "esno src/index.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:build": "pnpm run prepare && pnpm run build:play && pnpm run build:play:nuxt",
    "test:nuxt": "(cd test/fixtures/nuxt && npx nuxi dev)",
    "test:vite": "(cd test/fixtures/vite && npx vite dev)",
    "prepare": "pnpm -C playground run prepare && npm -C docs run prepare"
  },
  "dependencies": {
    "@unocss/reset": "^0.48.4",
    "@volar/vue-language-core": "^1.0.24",
    "acorn": "^8.8.1",
    "chroma-js": "^2.4.2",
    "consola": "^2.15.3",
    "csstype": "^3.1.1",
    "defu": "^6.1.1",
    "magic-string": "^0.27.0",
    "nanoid": "^4.0.0",
    "ohash": "^1.0.0",
    "postcss-custom-properties": "12.1.11",
    "postcss-dark-theme-class": "0.7.3",
    "postcss-nested": "^6.0.0",
    "recast": "^0.22.0",
    "scule": "^1.0.0",
    "style-dictionary-esm": "^1.1.1",
    "unbuild": "^1.1.1",
    "unplugin": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@nuxt/kit": "^3.0.0",
    "@nuxt/test-utils": "^3.0.0",
    "@nuxthq/studio": "^0.6.3",
    "@nuxtjs/color-mode": "^3.2.0",
    "@types/chroma-js": "^2.1.4",
    "@types/node": "^18.11.18",
    "@vue/test-utils": "^2.2.7",
    "eslint": "^8.32.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "js-confetti": "^0.11.0",
    "jsdom": "^21.1.0",
    "nitropack": "^1.0.0",
    "nodemon": "^2.0.20",
    "nuxi": "^3.0.0",
    "nuxt": "^3.0.0",
    "nuxt-component-meta": "^0.4.3",
    "playwright": "^1.29.2",
    "release-it": "^15.6.0",
    "rimraf": "^4.1.1",
    "rollup": "^3.10.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-inspect": "^0.7.14",
    "vitest": "^0.27.3",
    "vue": "^3.2.45",
    "webpack": "^5.75.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "axios": "0.25.0",
        "vue": "^3.2.45",
        "vite": "^4.0.0"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "@algolia/client-search",
        "nuxt",
        "vue"
      ]
    }
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "pnpm build"
      ]
    },
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
